<?xml version="1.0" encoding="UTF-8"?>

<project name="travis-2" default="default" basedir=".">
    <description>Builds, tests, and runs the project travis-2.</description>
    <import file="nbproject/build-impl.xml"/>

        <target name="-post-compile">
            <obfuscate>
                <fileset dir="${build.classes.dir}"/>
            </obfuscate>
        </target>
        
        <target name="run" depends="travis-2-impl.jar">
            <exec dir="bin" executable="launcher.exe">
                <arg file="${dist.jar}"/>
            </exec>
        </target>

   
</project>
